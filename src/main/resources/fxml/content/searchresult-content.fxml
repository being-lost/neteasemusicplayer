<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<TabPane fx:id="tabPaneSearchResult" prefHeight="344.0" prefWidth="509.0"
         stylesheets="@../../css/SearchResultStyle.css"
         fx:controller="controller.content.SearchResultContentController"
         tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" >
   <tabs>
      <Tab>
         <content>
            <StackPane>
               <children>
                  <BorderPane fx:id="borderPane">
                     <center>
                        <TableView fx:id="tableViewSong" onMouseClicked="#onClickedTableView" prefHeight="292.0" prefWidth="516.0" style="-fx-padding: 0;">
                           <columns>
                              <TableColumn fx:id="nameColumn" editable="false" maxWidth="-1.0" minWidth="200.0" prefWidth="200.0" resizable="false" text="歌名" />
                              <TableColumn fx:id="singerColumn" editable="false" maxWidth="-1.0" minWidth="100.0" prefWidth="100.0" resizable="false" text="歌手" />
                              <TableColumn fx:id="albumColumn" editable="false" maxWidth="-1.0" minWidth="100.0" prefWidth="100.0" resizable="false" text="专辑" />
                              <TableColumn fx:id="totalTimeColumn" editable="false" maxWidth="-1.0" minWidth="100.0" prefWidth="100.0" resizable="false" text="时长" />
                           </columns>
                           <placeholder>
                              <Label text=""/>
                           </placeholder>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                     </center>
                  </BorderPane>
                  <ProgressIndicator fx:id="progressIndicator" />
               </children>
            </StackPane>
         </content>
         <graphic>
            <HBox alignment="CENTER" prefHeight="35.0" prefWidth="50.0" spacing="3.0">
               <children>
                  <HBox fx:id="hBoxSong" alignment="CENTER" maxWidth="40.0" onMouseClicked="#onClickedHBoxSong" prefHeight="0.0" prefWidth="40.0" spacing="3.0">
                     <children>
                        <Label styleClass="labIndicator" text="歌曲" />
                     </children>
                  </HBox>
               </children>
            </HBox>
         </graphic>
      </Tab>
      <Tab>
         <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <Label layoutX="272.0" layoutY="148.0" text="暂无内容" />
               </children>
            </AnchorPane>
         </content>
         <graphic>
            <HBox alignment="CENTER" prefHeight="35.0" prefWidth="50.0" spacing="3.0">
               <children>
                  <HBox fx:id="hBoxSinger" alignment="CENTER" maxWidth="40.0" onMouseClicked="#onClickedHBoxSinger" prefHeight="0.0" prefWidth="40.0" spacing="3.0">
                     <children>
                        <Label styleClass="labIndicator" text="歌手" />
                     </children>
                  </HBox>
               </children>
            </HBox>
         </graphic>
      </Tab>
      <Tab>
         <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <Label layoutX="272.0" layoutY="148.0" text="暂无内容" />
               </children>
            </AnchorPane>
         </content>
         <graphic>
            <HBox alignment="CENTER" prefHeight="35.0" prefWidth="50.0" spacing="3.0">
               <children>
                  <HBox fx:id="hBoxAlbum" alignment="CENTER" maxWidth="40.0" onMouseClicked="#onClickedHBoxAlbum" prefHeight="0.0" prefWidth="40.0" spacing="3.0">
                     <children>
                        <Label styleClass="labIndicator" text="专辑" />
                     </children>
                  </HBox>
               </children>
            </HBox>
         </graphic>
      </Tab>
   </tabs>
</TabPane>
